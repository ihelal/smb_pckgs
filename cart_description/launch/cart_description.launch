<?xml version="1.0"?>
<launch>

  <arg name="robot_namespace" default="/"/>


  <arg name="world_name" default="worlds/empty.world"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    
    <arg name="world_name" value="$(arg world_name)"/> <!-- world_name is wrt GAZEBO_RESOURCE_PATH environment variable -->
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

   <param name="cart_description" command="$(find xacro)/xacro.py '$(find cart_description)/urdf/cart.urdf.xacro'"/>

  <node name="cart_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
   args="-urdf -param cart_description -model cart" />
</launch>
